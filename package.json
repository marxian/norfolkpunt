{
  "name": "norfolkpunt",
  "description": "Norfolk Punt Owners Association Site",
  "version": "1.0.0",
  "author": "Rupert Redington <rupert@neontribe.co.uk>",
  "dependencies": {
    "glob": "7.1.7",
    "gray-matter": "4.0.3",
    "image-size": "1.0.0",
    "netlify-cms-app": "2.15.20",
    "netlify-cms-oauth-provider-node": "2.0.0",
    "next": "11",
    "react": "17",
    "react-dom": "17",
    "react-image-lightbox": "5.1.1",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "responsive-loader": "2.3.0",
    "tachyons": "4.12.0"
  },
  "keywords": [
    "Norfolk Punt"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint"
    ]
  },
  "scripts": {
    "dev": "next",
    "prebuild": "node ./content/buildRegister.js",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '{*/**,.}/*.{js,json}'",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "localcms": "netlify-cms-proxy-server",
    "prepare": "husky install"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "netlify-cms-proxy-server": "1.3.18",
    "papaparse": "5.3.1",
    "prettier": "2.3.2"
  }
}
