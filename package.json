{
  "name": "norfolkpunt",
  "description": "Norfolk Punt Owners Association Site",
  "version": "1.0.0",
  "author": "Rupert Redington <rupert@neontribe.co.uk>",
  "dependencies": {
    "@mdx-js/loader": "1.4.5",
    "@next/mdx": "9.0.5",
    "gatsby-image": "2.2.18",
    "glob": "7.1.4",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.0.0",
    "lqip-loader": "2.2.0",
    "next": "9.0.5",
    "next-compose-plugins": "2.2.0",
    "next-mdx-frontmatter": "0.0.3",
    "next-optimized-images": "2.5.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "responsive-loader": "1.2.0",
    "sharp": "0.23.0"
  },
  "keywords": [
    "Norfolk Punt"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "data": "node ./extract.js && node ./buildIndex.js",
    "dev": "next",
    "prebuild": "yarn data",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '{*/**,.}/*.{js,json}'",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "eslint": "6.1.0",
    "eslint-config-react-app": "5.0.1",
    "eslint-plugin-flowtype": "4.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "husky": "3.0.7",
    "js-yaml": "3.13.1",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "sqlite3": "4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
